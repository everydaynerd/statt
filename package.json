{
  "name": "statt",
  "version": "1.0.0",
  "description": "A static site workflow that auto-reloads on file changes; synchronizes browsers; compiles, autoprefixes, minifies, and sourcemaps SCSS; concatenates and uglifies JS; provides an external URL for easy cross-device development.",
  "author": "Jamie Wilson",
  "license": "ISC",
  "scripts": {
    "start": "node config/statt.js",
    "clean": "NODE_ENV=clean node config/statt.js",
    "production": "NODE_ENV=production node config/statt.js"
  },
  "dependencies": {
    "browser-sync": "^2.16.0",
    "chalk": "~1.1.1",
    "handlebars": "~4.0.5",
    "metalsmith": "~2.1.0",
    "metalsmith-assets": "~0.1.0",
    "metalsmith-collections": "git+https://github.com/spacedawwwg/metalsmith-collections.git",
    "metalsmith-data-markdown": "0.0.3",
    "metalsmith-date-formatter": "~1.0.2",
    "metalsmith-drafts": "~0.0.1",
    "metalsmith-in-place": "~1.3.2",
    "metalsmith-layouts": "~1.4.2",
    "metalsmith-markdown": "~0.2.1",
    "metalsmith-permalinks": "~0.4.0",
    "metalsmith-sense-sass": "^1.1.0",
    "metalsmith-static": "0.0.5",
    "metalsmith-uglify": "~1.2.1",
    "metalsmith-watch": "~1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamiewilson/statt.git"
  }
}
